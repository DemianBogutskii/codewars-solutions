def task(w,n,c):
    na = ["James", "John", "Robert", "Michael", "William"]
    wa = ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday"]
    return f"It is {w} today, {na[wa.index(w)]}, you have to work, you must spray {n} trees and you need {n * c} dollars to buy liquid"
