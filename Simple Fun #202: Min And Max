#https://www.codewars.com/kata/58fd52b59a9f65c398000096/solutions/python

def min_and_max(l, d, x):
    for el in range(l, d+1):
        if sum([int(e) for e in str(el)]) == x:
            min = el
            break
    for el in range(d, l-1, -1):
        if sum([int(e) for e in str(el)]) == x:
            max = el
            break
    return [min,max]
